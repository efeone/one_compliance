 "html": "<div >\n{% if letter_head and not no_letterhead -%}\n<div class=\"letter-head\">{{ letter_head }}</div>\n{%- endif %}\n<div class=\"row section-break\" data-label=\"company_details\">\n    <hr>\n    <!--<h1 style=\"text-align:center\">Sales Invoice</h1>-->\n    <h2 style=\"text-align:center\">{{ doc.workflow_state or ''}}</h2>\n    <div class=\"row\">\n    <div class=\"col-xs-2 text-right\" style=\"background: #ffffff; font-size:12px;width:20%; color:\n    #4C5A67;\">\n    Company :<br><br><br>Address :\n</div>\n<div class=\"col-xs-4 text-left\" style=\"background: #ffffff; font-size:12px;width:29%\">\n    {{ doc.company }}<br>\n    {% if doc.company_address %}\n    {% set address = frappe.get_doc(\"Address\", doc.company_address) %}\n    {% if address %}\n    {{ address.address_line1 or '' }}<br>\n    {{ address.address_line2 or '' }}<br>\n    {{ address.city or '' }}, {{ address.state or '' }}<br>\n    {{ address.country or '' }}, {{ address.pincode or '' }}\n    {% endif %}\n    {% endif %}\n</div>\n<div class=\"col-xs-3 text-right\" style=\"background: #ffffff; font-size:12px;width:25%; color:\n    #4C5A67;\">\n    Date :<br>INV No :<br>Payment Due Date :<br>GSTIN :</div>\n    <div class=\"col-xs-3 text-left\" style=\"background: #ffffff; font-size:12px\">\n        <b>{{ doc.get_formatted(\"posting_date\") }}<br>\n        {{ doc.name }}<br>\n        {{ doc.due_date }}</b> <br>\n        {{ doc.company_gstin or '' }}</div>\n    </div><br>\n</div>\n<div class=\"row section-break\" data-label=\"customer_details\">\n<hr>\n<div class=\"row\">\n    <div class=\"col-xs-2 text-right\" style=\"background: #ffffff; font-size:12px;width:20%; color:\n    #4C5A67;\">\n    Customer Name :<br><br>Address :\n    </div>\n<div class=\"col-xs-4 text-left\" style=\"background: #ffffff; font-size:12px;width:28%\">\n    <b> {{ doc.customer_name }}</b><br><br>{{ doc.customer_address or '' }}\n</div>\n<div class=\"col-xs-3 text-right\" style=\"background: #ffffff; font-size:12px;width:25%; color:\n#4C5A67;\">\nGSTIN :</div>\n<div class=\"col-xs-3 text-left\" style=\"background: #ffffff; font-size:12px\">\n{{ doc.billing_address_gstin  or ''}} </div>\n<!--</div><br>-->\n</div>\n\n</div>\n<div class=\"row section-break\" data-label=\"\">\n<div class=\"col-xs-12 column-break\">\n<div data-fieldname=\"items\" data-fieldtype=\"Table\">\n<table class=\"table table-bordered table-condensed\" style=\"font-size:12px\">\n<thead>\n<tr>\n<th style=\"width: 40px\" class=\"table-sr\">Sr</th>\n\n<th style=\"width: 200px;\" class=\"\" data-fieldname=\"items\" data-\nfieldtype=\"Table\">\n\nItem</th>\n<th style=\"width: 40%;\" class=\"\" data-fieldname=\"items\"\n\ndata-fieldtype=\"Table\">\n\nDescription</th>\n<th style=\"width: 11%;\" class=\"\" data-fieldname=\"items\"\n\ndata-fieldtype=\"Table\">\n\nHSN/SAC </th>\n\n<th style=\"width: 80px;\" class=\"text-right\" data-\nfieldname=\"items\" data-fieldtype=\"Table\">\n\nRate</th>\n\n<th style=\"width: 80px;\" class=\"text-right\" data-\nfieldname=\"items\" data-fieldtype=\"Table\">\n\nAmount</th>\n\n</tr>\n</thead>\n<tbody>\n{% for item in doc.items %}\n<tr>\n<td class=\"table-sr\">{{item.idx}}</td>\n<td class=\"table-sr\">{{item.item_code}}</td>\n<td class=\"\" data-fieldname=\"items\" data-fieldtype=\"Table\">\n<div class=\"value\"><div>\n\n<div class=\"primary compact-item\">\n{{item.description or ''}}\n</div>\n</div>\n</td>\n\n<td class=\"table-sr\">{{item.gst_hsn_code or ''}}</td>\n\n<td class=\"text-right\" data-fieldname=\"items\" datqa-\nfieldtype=\"Table\">\n\n<div class=\"value\">\u20b9 {{item.rate or ''}}</div>\n</td>\n\n<td class=\"text-right\" data-fieldname=\"items\" data-\nfieldtype=\"Table\">\n\n<div class=\"value\">\u20b9 {{item.amount or ''}}</div>\n</td>\n</tr>\n{% endfor %}\n</tbody>\n</table>\n</div>\n</div>\n</div>\n<!--<div class=\"row section-break\" data-label=\"\">-->\n<!-- <div class=\"col-xs-4 column-break\">-->\n\n<!-- <div class=\"row data-field\" data-fieldname=\"total_qty\" data-fieldtype=\"Float\">-->\n<!-- <div class=\"col-xs-12\">-->\n<!-- <label> </label>-->\n<!-- </div>-->\n<!-- <div class=\"col-xs-12\"></div>-->\n<!-- </div>-->\n<!-- </div>-->\n<!-- <div class=\"col-xs-4 column-break\"></div>-->\n<!-- <div class=\"col-xs-4 column-break\">-->\n<!-- <div class=\"row\"></div>-->\n<!-- </div>-->\n<!--</div>-->\n<div class=\"row section-break\" data-label=\"Totals\">\n<div class=\"col-xs-6 column-break\"></div>\n<div class=\"col-xs-6 column-break\">\n\n<div class=\"row data-field\" data-fieldname=\"grand_total\" data-\nfieldtype=\"Currency\">\n\n<div class=\"col-xs-5\">\n<label>Total: </label>\n</div>\n<div class=\"col-xs-7 text-right value\">\n\n\u20b9 {{frappe.utils.fmt_money(doc.total)}}\n\n</div>\n</div>\n<div class=\"row data-field\" data-fieldname=\"discount\" data-fieldtype=\"Small Text\">\n    <div class=\"col-xs-7\">\n        {% if doc.discount_amount %}\n        <label>Additional Discount Amount: </label>\n        {% endif %}\n    </div>\n    <div class=\"col-xs-5 text-right value\">\n        {% if doc.discount_amount %}\n        \u20b9 {{ doc.discount_amount or ''}}\n        {% endif %}\n    </div>\n</div>\n\n<div class=\"row data-field\" data-fieldname=\"taxes\" data-fieldtype=\"Small Text\">\n    <div class=\"col-xs-7\">\n        {% for tax in doc.taxes %}\n        <label> {{ tax.description or ''}}@{{ tax.rate or ''}}</label>\n        {% endfor %}\n    </div>\n    <div class=\"col-xs-5 text-right value\">\n        {% for tax in doc.taxes %}\n        <label>\u20b9 {{ frappe.utils.fmt_money(tax.tax_amount or '')}}</label>\n        {% endfor %}\n    </div>\n</div>\n\n<div class=\"row important data-field\" data-fieldname=\"total_amount\" data-fieldtype=\"Currency\" style=\"font-size:12px;\">\n    <div class=\"col-xs-6 text-left\">\n        Total Amount : <br>\n        Amount In Words : <br>\n        Outstanding Amount : \n    </div>\n    <div class=\"col-xs-6 text-right\">\n        \u20b9 {{ frappe.utils.fmt_money(doc.grand_total) }}<br>\n        {{ frappe.utils.money_in_words(doc.grand_total) }}<br>\n       \u20b9 {{ frappe.utils.fmt_money(doc.rounded_total) }}\n    </div>\n    </div>\n</div>\n    <div class=\"row section-break\" data-label=\"\">\n        <div class=\"col-xs-4 column-break\">\n        <div class=\"row data-field\" data-fieldname=\"total_qty\" data-fieldtype=\"Float\">\n        <div class=\"col-xs-12\">\n            <label> </label>\n        </div>\n        <div class=\"col-xs-12\"></div>\n    </div>\n</div>\n<div class=\"col-xs-4 column-break\"></div>\n    <div class=\"col-xs-4 column-break\">\n        <div class=\"row\"></div>\n    </div>\n\n</div>\n<div class=\"row section-break\" data-label=\"Totals\" style=\"font-size:12px\">\n<div class=\"col-xs-6 column-break\"></div>\n<div class=\"col-xs-6 column-break\">\n<div class=\"row\"></div>\n</div>\n<div class=\"row data-field\" data-fieldname=\"terms\" data-fieldtype=\"Small Text\">\n\n<div class=\"col-xs-7\">\n{% if doc.terms %}\n<label>Terms &amp; Conditions: </label>\n{% endif %}\n</div>\n<div class=\"col-xs-5 text-right value\">\n{% if doc.terms %}\n\u20b9 {{ doc.terms }}\n{% endif %}\n</div>\n</div>\n</div>\n</div>\n<div style=\"border: 1px solid #fff; font-size: 12px;\">\n<h3 style=\"text-decoration: underline;\">Bank Details</h3>\n{% set bank_accounts = frappe.get_all(\"Bank Account\", fields=['bank_account_no', 'account_name',\n'bank', 'branch_code', 'custom_branch', 'company']) %}\n<table style=\"width: 100%; font-size: 12px;\">\n<tr>\n<td style=\"width: 20%;\">Account No :<br>Name :<br>Bank :<br>IFSC :<br>Branch :</td>\n<td style=\"width: 58%;\">\n    {% for bank_account in bank_accounts %}\n        {% if doc.company == bank_account.company %}\n             {{ bank_account.bank_account_no or '' }}<br>\n             {{ bank_account.account_name or '' }}<br>\n             {{ bank_account.bank or ''}}<br>\n             {{ bank_account.branch_code or '' }}<br>\n             {{ bank_account.custom_branch or '' }}<br>\n        {% endif %}\n    {% endfor %}\n</td>\n<td style=\"text-align: center;\">For CPAS Calicut<br><img src=\"/files/cpas_signature.png\"\nalt=\"img\">Authorized Signatory</td>\n</tr>\n</table>\n</div>\n<div id=\"footer-html\" class=\"visible-pdf\">\n{% if not no_letterhead and footer %}\n<div class=\"letter-head-footer\">\n{{ footer }}\n</div>\n{% endif %}\n</div>",
